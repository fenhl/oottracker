[Unit]
Description=Web server for oottracker.fenhl.net
After=network-online.target
Wants=network-online.target

[Service]
Restart=always
ExecStart=/opt/git/github.com/fenhl/oottracker/master/target/release/oottracker-web
User=fenhl
Group=fenhl
WorkingDirectory=/opt/git/github.com/fenhl/oottracker/master
EnvironmentFile=/opt/git/github.com/fenhl/oottracker/master/assets/web/env.txt

[Install]
WantedBy=multi-user.target
